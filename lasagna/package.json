{
  "name": "lasagna",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --no-hmr --configuration local",
    "start-prod": "ng serve --no-hmr --configuration production",
    "start-host": "ng serve --no-hmr --host 192.168.1.163 --port 4200 --disable-host-check",
    "prebuild": "node scripts/generate-ver.js && node scripts/inject-translations.js",
    "build": "ng build --base-href https://khripunovpp.github.io/lasagna/ --configuration development && cp ../docs/index.html ../docs/404.html",
    "build-vercel": "npm run config && npm run prebuild && ng build --configuration production",
    "config": "node scripts/set-env.js",
    "cleanup": "ng generate @angular/core:cleanup-unused-imports",
    "watch": "ng build --watch --configuration local",
    "test": "ng test",
    "po:import:ru": "i18next-conv -l ru -s public/i18n/ru.json -t public/i18n/ru.po",
    "po:import:pt": "i18next-conv -l pt -s public/i18n/pt.json -t public/i18n/pt.po",
    "po:import:en": "i18next-conv -l en -s public/i18n/en.json -t public/i18n/en.po",
    "po:export:ru": "i18next-conv -l ru -s public/i18n/ru.po -t public/i18n/ru.json",
    "po:export:pt": "i18next-conv -l pt -s public/i18n/pt.po -t public/i18n/pt.json",
    "po:export:en": "i18next-conv -l en -s public/i18n/en.po -t public/i18n/en.json",
    "po:import": "npm run po:import:ru && npm run po:import:pt && npm run po:import:en",
    "po:export": "npm run po:export:ru && npm run po:export:pt && npm run po:export:en",
    "i18n": "ngx-translate-extract --input ./src --output ./public/i18n/{en,pt,ru}.json --format json --sort",
    "inject-translations": "node scripts/inject-translations.js",
    "build-docs": "cd documentation && node build.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.2.1",
    "@angular/cdk": "20.2.0",
    "@angular/common": "20.2.1",
    "@angular/compiler": "20.2.1",
    "@angular/core": "20.2.1",
    "@angular/forms": "20.2.1",
    "@angular/material": "20.2.0",
    "@angular/platform-browser": "20.2.1",
    "@angular/platform-browser-dynamic": "20.2.1",
    "@angular/platform-server": "20.2.1",
    "@angular/router": "20.2.1",
    "@angular/service-worker": "20.2.1",
    "@angular/ssr": "20.2.0",
    "@colsen1991/ngx-translate-extract-marker": "3.0.1",
    "@ng-select/ng-select": "15.1.2",
    "@ngneat/overview": "6.1.1",
    "@ngx-translate/core": "16.0.4",
    "@ngx-translate/http-loader": "16.0.1",
    "@ngxpert/hot-toast": "4.1.2",
    "@sentry/angular": "9.29.0",
    "@sentry/cli": "2.43.0",
    "@zxing/browser": "0.1.5",
    "date-fns": "4.1.0",
    "decimal.js": "^10.6.0",
    "dexie": "4.0.11",
    "express": "4.21.2",
    "flexsearch": "0.8.158",
    "hammerjs": "2.0.8",
    "hotkeys-js": "3.13.9",
    "i18next-conv": "15.1.1",
    "jspdf": "3.0.1",
    "jspdf-autotable": "5.0.2",
    "lodash": "^4.17.21",
    "mathjs": "14.4.0",
    "ng2-charts": "8.0.0",
    "rxjs": "7.8.0",
    "tslib": "2.3.0",
    "zod": "3.24.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "20.2.0",
    "@angular/cli": "20.2.0",
    "@angular/compiler-cli": "20.2.1",
    "@types/chart.js": "2.9.41",
    "@types/express": "4.17.17",
    "@types/flexsearch": "0.7.6",
    "@types/hammerjs": "2.0.46",
    "@types/jasmine": "5.1.0",
    "@types/lodash": "^4.17.19",
    "@types/node": "18.18.0",
    "@vendure/ngx-translate-extract": "9.4.0",
    "colors": "1.4.0",
    "dotenv": "16.3.1",
    "gray-matter": "4.0.3",
    "jasmine-core": "5.6.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "marked": "15.0.8",
    "typescript": "5.8.3"
  }
}
